# Twitter API Configuration
TWITTER_API_KEY=your_twitter_api_key
TWITTER_API_SECRET=your_twitter_api_secret
TWITTER_BEARER_TOKEN=your_twitter_bearer_token

# DecentralGPT Configuration
# Available endpoints:
# - https://usa-chat.degpt.ai/api/v0/chat/completion
# - https://singapore-chat.degpt.ai/api/v0/chat/completion
# - https://korea-chat.degpt.ai/api/v0/chat/completion
DECENTRALGPT_ENDPOINT=https://usa-chat.degpt.ai/api/v0/chat/completion
DECENTRALGPT_PROJECT=DecentralGPT
DECENTRALGPT_MODEL=Llama3-70B
DECENTRALGPT_API_KEY=your_decentralgpt_api_key
# API Pricing Method: TOKEN or VIP
# TOKEN: $0.05 per million input tokens, $0.15 per million output tokens
# VIP: $3 per month, 1,000 conversations per day
DECENTRALGPT_PRICING_METHOD=TOKEN

# DeepBrainChain Configuration
DBC_RPC_URL=https://rpc.dbcwallet.io
DBC_CHAIN_ID=19880818
DBC_PRIVATE_KEY=your_wallet_private_key

# XAA Token Configuration
XAA_CONTRACT_ADDRESS=your_xaa_contract_address
PLATFORM_WALLET_ADDRESS=your_platform_wallet_address
MATCHING_ANALYSIS_COST=10 # Cost in XAA tokens for matching analysis

# Server Configuration
PORT=3000
NODE_ENV=development
